	Journal = {AJNR Am J Neuroradiol},
	Journal-Full = {AJNR. American journal of neuroradiology},
	Mesh = {Adult; Brain; Cerebrospinal Fluid; Humans; Magnetic Resonance Imaging; Male; Middle Aged; Reference Values; Sensitivity and Specificity},
	Month = {May},
	Number = {5},
