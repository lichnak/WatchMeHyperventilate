	Mesh = {Adult; Analysis of Variance; Behavior; Brain; Brain Injuries; Coma; Female; Functional Laterality; Glasgow Outcome Scale; Humans; Magnetic Resonance Imaging; Male; Middle Aged; Neurologic Examination; Prognosis; Prospective Studies; Treatment Outcome},
	Month = {Feb},
