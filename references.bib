	Journal = {Front Neurosci},
	Journal-Full = {Frontiers in neuroscience},
	Keywords = {MRI; classification; fMRI; intrinsic networks; unsupervised learning},
	Pages = {229},
	Pmc = {PMC4138493},
	Pmid = {25191215},
	Pst = {epublish},
	Title = {Deep learning for neuroimaging: a validation study},
	Volume = {8},
	Year = {2014},
	Bdsk-Url-1 = {http://dx.doi.org/10.3389/fnins.2014.00229}}

