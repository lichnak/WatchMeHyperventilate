	Pmc = {PMC2276735},
	Pmid = {17659998},
	Pst = {ppublish},
	Title = {Symmetric diffeomorphic image registration with cross-correlation: evaluating automated labeling of elderly and neurodegenerative brain},
	Volume = {12},
	Year = {2008},
