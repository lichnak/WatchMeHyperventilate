	Keywords = {Alzheimer's disease; Classification; Multiple instance learning; Structural MR imaging},
	Mesh = {Aged; Algorithms; Alzheimer Disease; Artificial Intelligence; Brain; Female; Humans; Image Enhancement; Image Interpretation, Computer-Assisted; Magnetic Resonance Imaging; Male; Pattern Recognition, Automated; Reproducibility of Results; Sensitivity and Specificity},
